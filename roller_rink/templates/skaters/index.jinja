{% extends 'base.jinja' %}

{% block content %}
<h1>{{ title }}</h1>

{% for skater in skaters %}
{% if skater.premium_member %}
    <p>
        <b><a href="/skaters/{{ skater.id }}">{{ skater.full_name }}</a>:</b> PREMIUM MEMBER
    </p>
    <hr>

{% else %}

    <p>
        <b><a href="/skaters/{{ skater.id }}">{{ skater.full_name }}</a></b>
    </p>
    <hr>

{% endif %}
{% endfor %}

<p><a href="/skaters/new">Register new skater</a></p>

{% endblock %}